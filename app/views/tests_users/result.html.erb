<h1>Тест  <%=@test_user.test.title %> завершен</h1>

<h2><%=  @test_user.finish_test  %></h2>

<% if @test_user.test_result > 85  %>
  <h3 class='done'><%=@test_user.test_result%><%="%"%></h3>
<% else @test_user.test_result < 85  %>
  <h4 class='failed'><%=@test_user.test_result%><%="%"%></h4>
<% end %>

<%= link_to 'Назад', tests_path %>